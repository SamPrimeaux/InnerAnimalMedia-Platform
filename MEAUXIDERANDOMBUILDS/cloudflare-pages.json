{
  "name": "iaccess-platform",
  "production_branch": "main",
  "build_command": "",
  "output_directory": ".",
  "root_directory": "/",
  "framework": "static",
  "compatibility_date": "2024-01-01",
  "routes": [
    {
      "pattern": "/api/*",
      "service": "iaccess-api"
    },
    {
      "pattern": "/*",
      "file": "/index.html"
    }
  ],
  "redirects": [
    {
      "from": "/dashboard",
      "to": "/dashboard/index.html",
      "statusCode": 301
    },
    {
      "from": "/dashboard/overview",
      "to": "/dashboard/index.html",
      "statusCode": 301
    },
    {
      "from": "/workflows",
      "to": "/workflows.html",
      "statusCode": 301
    },
    {
      "from": "/workers",
      "to": "/workers.html",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
